{
  "name": "@magsag/cli",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "magsag": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts --external @magsag/*",
    "prepare": "npm run build",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest --run --config vitest.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "pnpm run build"
  },
  "dependencies": {
    "@magsag/core": "workspace:*",
    "@magsag/schema": "workspace:*",
    "@magsag/governance": "workspace:*",
    "@magsag/observability": "workspace:*",
    "@magsag/mcp-client": "workspace:*",
    "@magsag/catalog-mcp": "workspace:*",
    "@magsag/mcp-server": "workspace:*",
    "@magsag/runner-adk": "workspace:*",
    "@magsag/runner-claude-agent": "workspace:*",
    "@magsag/runner-claude-cli": "workspace:*",
    "@magsag/runner-codex-cli": "workspace:*",
    "@magsag/runner-openai-agents": "workspace:*",
    "@magsag/manager": "workspace:*",
    "@magsag/specialist": "workspace:*",
    "@magsag/worktree": "workspace:*",
    "@oclif/core": "3.27.0",
    "execa": "^8.0.1",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.21.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artificial-intelligence-first/magsag.git",
    "directory": "packages/cli"
