{
  "name": "@magsag/core",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@magsag/shared-logging": "^0.1.0",
    "pidusage": "^3.0.2"
  },
  "devDependencies": {
    "@magsag/schema": "^2.0.0-alpha.0",
    "@types/pidusage": "^2.0.5"
  },
  "peerDependencies": {
    "@magsag/schema": "^2.0.0-alpha.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --external pidusage --external @magsag/*",
