{
  "name": "@magsag/manager",
  "version": "2.0.0-alpha.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./planner": {
      "import": "./dist/planner.js",
      "types": "./src/planner.ts"
    },
    "./auto-tune": {
      "import": "./dist/auto-tune.js",
      "types": "./src/auto-tune.ts"
    },
    "./providers": {
      "import": "./dist/providers.js",
      "types": "./src/providers.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/planner.ts src/auto-tune.ts src/providers.ts --dts --format esm",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest --run --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@magsag/core": "workspace:*"
  },
  "license": "Apache-2.0"
}
