{
  "name": "@magsag/server",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts && node scripts/generate-openapi.mjs",
    "lint": "eslint \"src/**/*.ts\"",
    "prepack": "pnpm run build",
    "prepare": "npm run build",
    "test": "vitest --run --config vitest.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "smoke": "node scripts/rate-limit-smoke.mjs"
  },
  "dependencies": {
    "@hono/node-server": "^1.7.0",
    "@magsag/core": "workspace:*",
    "@magsag/observability": "workspace:*",
    "@magsag/schema": "workspace:*",
    "hono": "^4.10.5",
    "ws": "^8.18.0",
    "zod-to-json-schema": "^3.25.0"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artificial-intelligence-first/magsag.git",
    "directory": "packages/server"
  },
  "bugs": {
    "url": "https://github.com/artificial-intelligence-first/magsag/issues"
  },
  "homepage": "https://github.com/artificial-intelligence-first/magsag#readme",
  "license": "MIT"
}
