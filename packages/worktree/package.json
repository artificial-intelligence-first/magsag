{
  "name": "@magsag/worktree",
  "version": "2.0.0-alpha.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./manager": {
      "import": "./dist/manager.js",
      "types": "./src/manager.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup --config tsup.config.ts --external @magsag/*",
    "lint": "eslint \"src/**/*.ts\"",
    "prepack": "pnpm run build",
    "prepare": "npm run build",
    "test": "vitest --run --config vitest.config.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@magsag/schema": "^2.0.0-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artificial-intelligence-first/magsag.git",
    "directory": "packages/worktree"
  },
  "bugs": {
    "url": "https://github.com/artificial-intelligence-first/magsag/issues"
  },
  "homepage": "https://github.com/artificial-intelligence-first/magsag#readme",
  "license": "MIT"
}
